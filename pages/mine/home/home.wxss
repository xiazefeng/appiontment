/* pages/mine/home/home.wxss */
.UCenter-bg {
  background-image: url(https://yunxunjg-yunmall.oss-cn-hangzhou.aliyuncs.com/MaiaSalon_logo_1.jpg);
  background-size: cover;
  background-position: center center;
  height: 550rpx;
  display: flex;
  justify-content: center;
  padding-top: 80rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg image {
  width: 200rpx;
  height: 200rpx;
}

.UCenter-bg .gif-wave{
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  width: 750rpx;
  height: 300rpx;
}
.cu-line-height{
  line-height: 40rpx !important;
}
.my-title{
  display: flex;
  align-items: center;
  flex-direction: row ;
  justify-content: space-between;
}
.custom-class{
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.1)
}
